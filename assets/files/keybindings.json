// Place your key bindings in this file to override the defaults
[
  {
    "command": "notebook.execute",
    "key": "alt+enter"
  },
  {
    "command": "-cmake.debugTarget",
    "key": "shift+f5",
    "when": "cmake:enableFullFeatureSet && inCMakeProject && !cmake:hideDebugCommand && !inDebugMode"
  },
  {
    "command": "cmake.launchTarget",
    "key": "ctrl+enter",
    "when": "cmake:enableFullFeatureSet && !cmake:hideDebugCommand && !inDebugMode"
  },
  {
    "command": "-cmake.launchTarget",
    "key": "ctrl+shift+f5",
    "when": "cmake:enableFullFeatureSet && !cmake:hideDebugCommand && !inDebugMode"
  },
  {
    "command": "cmake.buildWithTarget",
    "key": "f7",
    "when": "cmake:enableFullFeatureSet && !cmake:hideBuildCommand"
  },
  {
    "command": "-cmake.buildWithTarget",
    "key": "shift+f7",
    "when": "cmake:enableFullFeatureSet && !cmake:hideBuildCommand"
  },
  {
    "command": "cmake.build",
    "key": "ctrl+shift+enter",
    "when": "cmake:enableFullFeatureSet && !cmake:hideBuildCommand"
  },
  {
    "command": "-cmake.build",
    "key": "f7",
    "when": "cmake:enableFullFeatureSet && !cmake:hideBuildCommand"
  },
  {
    "command": "python.execInTerminal-icon",
    "key": "cmd+enter"
  },
  {
    "command": "search.action.focusNextSearchResult",
    "key": "f9",
    "when": "hasSearchResult || inSearchEditor"
  },
  {
    "command": "-search.action.focusNextSearchResult",
    "key": "f4",
    "when": "hasSearchResult || inSearchEditor"
  },
  {
    "command": "search.action.focusPreviousSearchResult",
    "key": "f8",
    "when": "hasSearchResult || inSearchEditor"
  },
  {
    "command": "-search.action.focusPreviousSearchResult",
    "key": "shift+f4",
    "when": "hasSearchResult || inSearchEditor"
  },
  {
    "command": "workbench.action.toggleSidebarVisibility",
    "key": "cmd+shift+e",
    "when": "activeViewlet == 'workbench.view.explorer'"
  },
  {
    "command": "workbench.view.explorer",
    "key": "cmd+shift+e",
    "when": "activeViewlet != 'workbench.view.explorer'"
  },
  {
    "command": "workbench.action.toggleSidebarVisibility",
    "key": "cmd+f",
    "when": "activeViewlet == 'workbench.view.search'"
  },
  {
    "command": "workbench.view.search",
    "key": "cmd+f",
    "when": "activeViewlet != 'workbench.view.search'"
  },
  {
    "command": "workbench.action.toggleSidebarVisibility",
    "key": "cmd+g",
    "when": "activeViewlet == 'workbench.view.scm'"
  },
  {
    "command": "workbench.view.scm",
    "key": "cmd+g",
    "when": "activeViewlet != 'workbench.view.scm'"
  },
  {
    "command": "workbench.action.toggleSidebarVisibility",
    "key": "cmd+shift+d",
    "when": "activeViewlet == 'workbench.view.debug'"
  },
  {
    "command": "workbench.view.debug",
    "key": "cmd+shift+d",
    "when": "activeViewlet != 'workbench.view.debug'"
  },
  {
    "command": "workbench.action.toggleSidebarVisibility",
    "key": "cmd+shift+x",
    "when": "activeViewlet == 'workbench.view.extensions'"
  },
  {
    "command": "workbench.view.extensions",
    "key": "cmd+shift+x",
    "when": "activeViewlet != 'workbench.view.extensions'"
  },
  {
    "command": "workbench.action.openRecent",
    "key": "cmd+o"
  },
  {
    "command": "-workbench.action.openRecent",
    "key": "ctrl+r"
  },
  {
    "command": "workbench.action.focusLeftGroup",
    "key": "cmd+h"
  },
  {
    "command": "workbench.action.focusRightGroup",
    "key": "cmd+l"
  },
  {
    "command": "workbench.files.action.focusFilesExplorer",
    "key": "cmd+e"
  },
  {
    "command": "workbench.action.focusFirstEditorGroup",
    "key": "cmd+k"
  },
  {
    "command": "workbench.action.terminal.focus",
    "key": "cmd+j"
  },
  {
    "command": "workbench.action.terminal.toggleTerminal",
    "key": "shift+cmd+t",
    "when": "terminal.active"
  },
  {
    "command": "-workbench.action.terminal.toggleTerminal",
    "key": "ctrl+`",
    "when": "terminal.active"
  },
  {
    "key": "shift+cmd+o",
    "command": "workbench.action.files.openFolder"
  },
  {
  "key": "cmd+shift+enter",                 // or any key you prefer
  "command": "workbench.action.terminal.sendSequence",
  "args": { "text": "\u001b[A\u000D" }, // ↑ then Enter
},
{
  "key": "cmd+shift+r",                 // or any key you prefer
  "command": "workbench.action.terminal.sendSequence",
  "args": { "text": "make\u000D" }, // ↑ then Enter
},
{
  "key": "cmd+/",
  "command": "runCommands",
  "args": {
    "commands": [
      "editor.action.commentLine",
      "cursorDown"
    ]
  },
  "when": "editorTextFocus && !editorReadonly"
}
]